{
  "name": "qmailo-frontend",
  "version": "1.2.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run ng -- serve --proxy-config proxy.conf.json --aot",
    "start:de": "npm run start -- --i18n-file=src/i18n/messages.de.xlf --locale=de --i18n-format=xlf",
    "start:sw": "npm run build && npm run start:liteserver",
    "start:liteserver": "lite-server -c liteserver_config.js",
    "build": "ng build --prod",
    "build:en": "npm run build -- --output-path=qmailo-frontend_v%npm_package_version%_en",
    "build:de": "npm run build -- --i18n-file=src/i18n/messages.de.xlf --locale=de --i18n-format=xlf --output-path=qmailo-frontend_v%npm_package_version%_de",
    "release": "npm run build:en && npm run build:de",
    "i18n": "ng xi18n --output-path src/i18n --locale de && xliffmerge --profile xliffmerge.json de",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.9",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "5.2.9",
    "@angular/compiler": "5.2.9",
    "@angular/core": "5.2.9",
    "@angular/forms": "5.2.9",
    "@angular/http": "5.2.9",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "5.2.9",
    "@angular/platform-browser-dynamic": "5.2.9",
    "@angular/router": "5.2.9",
    "@angular/service-worker": "5.2.9",
    "@ngrx/effects": "^5.2.0",
    "@ngrx/entity": "^5.2.0",
    "@ngrx/store": "^5.2.0",
    "@ngrx/store-devtools": "^5.2.0",
    "core-js": "^2.5.5",
    "rxjs": "^5.5.9",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "1.7.4",
    "@angular/compiler-cli": "5.2.9",
    "@angular/language-service": "5.2.9",
    "@ngrx/schematics": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.105",
    "codelyzer": "^4.2.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.3.0",
    "ngx-i18nsupport": "^0.11.1",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "2.6.2"
  }
}
